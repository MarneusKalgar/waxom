include ../partials/carousel.pug
include ../partials/purchaseModal.pug

block variables
doctype html
html(lang='ru')
	head: include ../partials/head.pug
	body
		include ../partials/svgicons.pug
		+purchaseModal()
		unless debug === false
			include ../partials/debug.pug
		div.all
			div.all__header
				block header
			div.all__body
				block body
			div.all__footer
				block footer
		include ../partials/popups.pug
		include ../partials/scripts.pug
